<% provide(:title, 'All users') %>
<h1>Users</h1>
<%= form_tag users_path, :method => 'get', :id => "users_search" do %>
  <p>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search", class: "btn btn-primary", :name => nil %>
  </p>
<% end %>
<p><%=link_to 'Clear', users_path %></p>

<%= will_paginate %>
<ul class="users">
  <%= render @users %>
</ul>
<%= will_paginate %>